{
  "name": "server",
  "version": "0.5.0",
  "private": true,
  "license": "MIT",
  "main": "dist/server.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './dist/server.js' --watch './dist'",
    "start": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "start-prod": "npm-run-all clean build && node ./dist/server.js",
    "lint": "tslint --project .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.0",
    "codelyzer": "^4.0.1",
    "glob": "^7.1.3",
    "nodemon": "^1.18.5",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^1.3.0",
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.1.0",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.2",
    "http-auth": "^3.2.3",
    "typescript": "^3.1.3"
  }
}
