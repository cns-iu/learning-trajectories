{
  "$schema": "../node_modules/ng-packagr/package.schema.json",
  "name": "learning-trajectories-database",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "webpack --config webpack.offline.config.js && node --max-old-space-size=8192 build/server",
    "build": "yarn build-offline-tools && yarn build-worker",
    "build-angular-package": "ng-packagr -p package.json",
    "build-worker": "webpack --config webpack.worker.config.js",
    "build-offline-tools": "webpack --config webpack.offline.config.js",
    "build-database": "yarn build-offline-tools && yarn read-raw-data && yarn create-database && rm -rf ../../raw-data/db && yarn import-database && yarn import-database",
    "read-raw-data": "node build/read-raw-data",
    "build-sci2-data": "rimraf ../../raw-data/sci2 && mkdir ../../raw-data/sci2 && node src/loader/build-sci2-data.js",
    "create-database": "node build/create-database",
    "import-database": "mkdir -p ../../raw-data/db && node build/import-database"
  },
  "private": true,
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts",
      "languageLevel": [
        "es2017",
        "dom",
        "esnext"
      ]
    }
  },
  "dependencies": {
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@ngx-dino/core": "^0.0.0",
    "apollo-angular": "^1.0.1",
    "apollo-errors": "^1.7.1",
    "apollo-link": "^1.2.1",
    "apollo-link-webworker": "^0.1.3",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "dotenv-webpack": "^1.5.5",
    "graphql": "^0.13.2",
    "graphql-server-express": "^1.3.4",
    "graphql-subscriptions": "^0.5.6",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^2.23.1",
    "http": "^0.0.0",
    "immutable": "^3.8.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.0",
    "morgan": "^1.8.1",
    "path": "^0.12.7",
    "rxdb": "^7.3.0",
    "rxjs": "^5.5.2",
    "start-server-webpack-plugin": "^2.2.5",
    "subscriptions-transport-ws": "^0.9.7",
    "webpack-node-externals": "^1.6.0",
    "xlsx": "^0.12.6",
    "zipcodes": "^6.0.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.1",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "codelyzer": "^4.0.1",
    "csv-write-stream": "^2.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.2.0",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "unidecode": "^0.1.8",
    "webpack": "^3.10.0",
    "worker-loader": "^1.1.1"
  },
  "resolutions": {
    "graphql": "^0.12.3"
  }
}
